"87","88","89","90"],"import React, { Component } from 'react'\nimport {  MDBContainer, MDBRow, MDBCol, MDBIcon, MDBBtn, MDBInput } from \"mdb-react-ui-kit\";\nimport {Jumbotron,Container} from 'react-bootstrap'\nimport '../SafetyMeasures/safetyMeasures.css'\n\nclass Contact extends Component {\n  render() {\n    return (\n\n       <React.Fragment>\n\n            <Jumbotron fluid>\n                <Container>\n                    <h2>صنايعي خبره : اجراءات السلامه</h2>\n                    <p>\n                    صنايعي خبره هو بوابة اى حرفه \n                    </p>\n                    <hr/>\n                </Container>\n            </Jumbotron>\n\n            <div className=\"safetySec\">\n                <Container>\n                    <h4>إجراءات السلامة في خدمات شركة \"صنايعي خبره \":</h4>\n                    <br/>\n                    <h5>مع كل الكلينرز (عاملات النظافة) والفنيين:</h5>\n                    <ul>\n                        <li>كل كلينر أو فني سيأتون لديكم مرتديين ماسك طبي وجوانتي طبي و over shoes</li>\n                        <li>بمجرد وصولهم لديكم سيقومون بغسل ايديهم جيدا بمياه وصابون.</li>\n                        <li>تم تدريب كل منهم على كيفية التعامل في منازل العملاء لمنع العدوى.</li>\n                        <li>تم تدريب جميع الكلينرز والفنيين على إستخدام أساليب الوقاية وكيفية حماية نفسهم من الفيروس.</li>\n                    </ul>\n                    <br/>\n                    <h5>خدمة تطهير المنزل:</h5>\n                    <ul>\n                        <li>تم إضافة خدمة التطهير مجانا إلى كل زيارات النظافة المنزلية بإستخدام أقراص KLORSEPT وبإستخدام كلور تنظيف.</li>\n                        <li>ستاتي الكلينر ومعها مواد التطهير اللازمة.</li>\n                    </ul>\n                    <br/>\n                    <h5>علشان تساعدنا في تنفيذ اجراءت السلامة، رجاء القيام بالأتي:</h5>\n                    <ul>\n                        <li>التأكد من غسل الكلينر أو الفني أيديهم قبل البدء في العمل في مكان من بيتك.</li>\n                        <li>التأكد من ارتدائهم الماسك قبل وخلال العمل، وإن لم يتوفر لدى العامل الماسك، نوصي بأن يكون لديك عدد من الماسكات في منزلك و ابلاغنا بالمشكلة فورا علي 19812</li>\n                        <li>تحضير جوانتي طبي لك وللكلينر أو الفني إحتياطي. إذا لم يكن هناك جوانتي رجاء ابلاغنا فورا علي 19812</li>\n                        <li>متابعة كافة الأماكن التي تواجد فيها الفني أو الكلينر والتأكد من تطهيرها بعد انتهائهم من عملهم</li>\n                        <li>ترك مسافة لا تقل عن مترين بينك وبين العامل الفني أو عامل النظافة طوال وقت تواجده بمنزلك</li>\n                        <li>ولو تحبوا انهم يستخدموا أي أساليب وقاية لديكم رجاء طلب ذلك منهم</li>\n                    </ul>\n                </Container>\n\n                \n            </div>\n\n       </React.Fragment>\n      \n    );\n  }\n}\n\nexport default Contact\n","D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\ScrollToTop\\ScrollToTop.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Navbartop\\Navbartop.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\NavBar\\NavBar.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Home\\Home.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\About\\About.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Contact\\Contact.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Services\\Services.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Footer\\footer.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Registration\\Craftsman\\Signup_Craftsman.js",["91","92","93","94","95","96"],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Registration\\Customer\\Signup_Customer.js",[],"D:\\samar\\react\\samar\\graduation-project-master\\src\\Components\\Registration\\Login\\Login.js",["97","98","99"],"import React, { useContext, useState, creat